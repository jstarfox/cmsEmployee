n></div>\n          <button class = \"btn btn-primary\" @click=\"handlePage\"> {{ editMode ? \"Edit\" : \"Add\"}} </button>\n          </div>\n        </div>\n</template>\n<script>\nimport { mapActions } from 'vuex';\n\nexport default {\n  data() {\n    return {\n      page: {\n        name: null,\n        slug: null,\n        content: null\n      }\n    }\n  },\n  computed: {\n    editMode(){\n      return this.$route.params[\"op\"] == \"edit\";\n    }\n  },\n\n  methods: {\n    ...mapActions([\"addPage\", \"editPage\"]),\n    handlePage() {\n      if(this.editMode) {\n        this.editPage(this.page)\n      }\n      else {\n        this.addPage(this.page)\n        console.log(this.page.name)\n      }\n      this.$router.push(\"/admin/pages\")\n    },\n    handleBack(){\n            this.$router.push(\"/admin/pages\")\n    }\n  },\n  created(){\n    if(this.editMode){\n      Object.assign(this.page, this.$store.getters.pageById(this.$route.params[\"id\"]))\n    }\n  }\n};\n</script>"]}]}